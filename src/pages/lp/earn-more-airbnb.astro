            line-height: 1.7;
            margin-bottom: 1.5rem;
        }

        .lp-urgency .lp-btn-primary {
            background: var(--lp-green);
        }

        .lp-urgency .lp-btn-primary:hover {
            background: var(--lp-green-dark);
        }

        /* ─── FINAL CTA EXTRAS ─── */
        .lp-final-offer-label {
            font-weight: 600;
            font-size: 1rem;
            opacity: 1 !important;
            margin-bottom: 0.75rem !important;
        }

        .lp-final-reminder {
            margin-top: 1.5rem;
            padding: 1rem 1.25rem;
            background: rgba(255,255,255,0.1);
            border-radius: 8px;
            border-left: 3px solid var(--lp-accent);
        }

        .lp-final-reminder p {
            font-size: 0.9rem;
            opacity: 0.9;
            margin-bottom: 0 !important;
        }

        /* ─── ANIMATIONS ─── */
        @keyframes fadeInUp {
            from { opacity: 0; transform: translateY(30px); }
            to { opacity: 1; transform: translateY(0); }
        }

        @keyframes fadeInRight {
            from { opacity: 0; transform: translateX(30px); }
            to { opacity: 1; transform: translateX(0); }
        }

        /* Scroll-triggered animations */
        .reveal {
            opacity: 0;
            transform: translateY(20px);
            transition: opacity 0.6s ease, transform 0.6s ease;
        }

        .reveal.visible {
            opacity: 1;
            transform: translateY(0);
        }

        /* ─── RESPONSIVE ─── */
        @media (max-width: 968px) {
            .lp-hero-inner {
                grid-template-columns: 1fr;
                text-align: center;
            }

            .lp-hero-content h1 {
                font-size: 3rem;
            }

            .lp-hero-stats {
                justify-content: center;
                grid-column: auto;
                grid-row: auto;
            }

            .lp-hero-buttons {
                justify-content: center;
            }

            .lp-hero-video {
                grid-column: auto;
                grid-row: auto;
            }

            .lp-pain-grid {
                grid-template-columns: 1fr;
            }

            .lp-case-grid {
                grid-template-columns: 1fr;
                gap: 2.5rem;
            }

            .lp-how-steps {
                grid-template-columns: 1fr;
            }

            .lp-step:not(:last-child)::after {
                display: none;
            }

            .lp-compare-table {
                font-size: 0.85rem;
            }

            .lp-compare-header > div,
            .lp-compare-row > div {
                padding: 0.7rem 0.75rem;
            }

            .lp-services-grid {
                grid-template-columns: 1fr 1fr;
            }

            .lp-final-inner {
                grid-template-columns: 1fr;
                text-align: center;
            }

            .lp-final-benefits {
                align-items: center;
            }

            .lp-whoisfor-grid {
                grid-template-columns: 1fr;
                max-width: 500px;
                margin: 0 auto;
            }

            .lp-guarantee-grid {
                grid-template-columns: 1fr;
                max-width: 500px;
                margin: 0 auto;
            }
        }

        @media (max-width: 768px) {
            .lp-hero {
                padding: 3rem 1.5rem 2rem;
            }

            .lp-hero-content h1 {
                font-size: 1.75rem;
                margin-bottom: 0.25rem;
            }

            .lp-hero-subtitle {
                font-size: 1rem;
                margin-bottom: 0.5rem;
            }

            .lp-hero-description {
                display: none;
            }

            .lp-hero-stats {
                flex-direction: row;
                gap: 1rem;
                margin-bottom: 0;
                margin-top: -0.5rem;
            }

            .lp-hero-video {
                margin-top: -0.5rem;
            }

            .lp-stat:not(.badge-stat) {
                margin-top: 50px;
            }

            .lp-stat-number {
                font-size: 1.2rem;
                margin-bottom: 2px;
                line-height: 1;
            }

            .lp-stat-label {
                font-size: 0.7rem;
            }

            .badge-stat {
                transform: translateY(-39px);
            }

            .lp-badge-img {
                height: 78px;
            }

            .lp-hero-buttons {
                margin-top: 0.75rem;
                margin-bottom: 0;
            }

            .lp-btn-primary {
                padding: 1rem 2rem;
                font-size: 1rem;
                box-shadow: 0 4px 15px rgba(117, 155, 143, 0.4);
                text-align: center;
                justify-content: center;
            }

            .lp-btn-primary::after {
                display: none;
            }
        }

        @media (max-width: 576px) {
            .lp-hero { padding: 3rem 1.25rem 2rem; }
            .lp-pain,
            .lp-lead-panel,
            .lp-case,
            .lp-how,
            .lp-compare,
            .lp-services,
            .lp-whoisfor,
            .lp-guarantee,
            .lp-urgency,
            .lp-faq,
            .lp-final-cta { padding: 3.5rem 1.25rem; }

            .lp-header { padding: 0.6rem 1rem; }
            .lp-header-phone { display: none; }

            .lp-btn-primary {
                width: 100%;
                justify-content: center;
            }

            .lp-services-grid {
                grid-template-columns: 1fr;
            }

            .lp-compare-header,
            .lp-compare-row {
                grid-template-columns: 1.4fr 1fr 1fr;
            }

            .lp-compare-feature,
            .lp-compare-nurture,
            .lp-compare-others {
                font-size: 0.8rem;
            }

            .lp-case-line { font-size: 0.88rem; }
        }

        /* Performance */
        .lp-lead-panel,
        .lp-case,
        .lp-how,
        .lp-compare,
        .lp-services,
        .lp-whoisfor,
        .lp-guarantee,
        .lp-urgency,
        .lp-faq {
            content-visibility: auto;
            contain-intrinsic-size: auto 500px;
        }

        /* Reduced motion */
        @media (prefers-reduced-motion: reduce) {
            *, *::before, *::after {
                animation-duration: 0.01ms !important;
                transition-duration: 0.01ms !important;
            }
        }
    </style>
</head>
