        }

        /* ‚îÄ‚îÄ‚îÄ WHO THIS IS FOR ‚îÄ‚îÄ‚îÄ */
        .lp-whoisfor {
            padding: 5rem 2rem;
            background: var(--lp-warm-white);
        }

        .lp-whoisfor-grid {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            gap: 1.5rem;
        }

        .lp-persona-card {
