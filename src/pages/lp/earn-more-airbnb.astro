            }

            .lp-compare-table {
                font-size: 0.85rem;
            }

            .lp-compare-header > div,
            .lp-compare-row > div {
                padding: 0.7rem 0.75rem;
            }

            .lp-services-grid {
                grid-template-columns: 1fr 1fr;
            }

            .lp-final-inner {
                grid-template-columns: 1fr;
                text-align: center;
            }

            .lp-final-benefits {
                align-items: center;
            }

            .lp-whoisfor-grid {
                grid-template-columns: 1fr;
                max-width: 500px;
                margin: 0 auto;
            }

            .lp-guarantee-grid {
                grid-template-columns: 1fr;
                max-width: 500px;
                margin: 0 auto;
            }
        }

        @media (max-width: 576px) {
            .lp-hero { padding: 3rem 1.25rem 2rem; }
            .lp-pain,
            .lp-lead-panel,
            .lp-case,
            .lp-how,
            .lp-compare,
            .lp-services,
            .lp-whoisfor,
            .lp-guarantee,
            .lp-urgency,
            .lp-faq,
            .lp-final-cta { padding: 3.5rem 1.25rem; }

            .lp-header { padding: 0.6rem 1rem; }
            .lp-header-phone { display: none; }

            .lp-hero-cta-group {
                flex-direction: column;
            }

            .lp-btn-primary {
                width: 100%;
                justify-content: center;
            }

            .lp-btn-secondary {
                width: 100%;
                justify-content: center;
            }

            .lp-services-grid {
                grid-template-columns: 1fr;
            }

            .lp-compare-header,
            .lp-compare-row {
                grid-template-columns: 1.4fr 1fr 1fr;
            }

            .lp-compare-feature,
            .lp-compare-nurture,
            .lp-compare-others {
                font-size: 0.8rem;
            }

            .lp-case-card { padding: 1.5rem; }
        }

        /* Performance */
        .lp-pain,
        .lp-lead-panel,
        .lp-case,
        .lp-how,
        .lp-compare,
        .lp-services,
        .lp-whoisfor,
        .lp-guarantee,
        .lp-urgency,
        .lp-faq {
            content-visibility: auto;
            contain-intrinsic-size: auto 500px;
        }

        /* Reduced motion */
        @media (prefers-reduced-motion: reduce) {
            *, *::before, *::after {
                animation-duration: 0.01ms !important;
                transition-duration: 0.01ms !important;
            }
        }
    </style>
