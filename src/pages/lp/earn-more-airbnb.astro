        .lp-urgency p {
            font-size: 1rem;
            opacity: 0.85;
            line-height: 1.7;
            margin-bottom: 1.5rem;
        }

        .lp-urgency .lp-btn-primary {
            background: var(--lp-green);
            box-shadow: 0 4px 20px rgba(117,155,143,0.4);
        }

        .lp-urgency .lp-btn-primary:hover {
            background: var(--lp-green-dark);
        }

        /* ‚îÄ‚îÄ‚îÄ FINAL CTA EXTRAS ‚îÄ‚îÄ‚îÄ */
        .lp-final-offer-label {
            font-weight: 600;
            font-size: 1rem;
            opacity: 1 !important;
            margin-bottom: 0.75rem !important;
        }

        .lp-final-reminder {
