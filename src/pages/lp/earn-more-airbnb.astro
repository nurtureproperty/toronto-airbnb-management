            box-shadow: 0 8px 24px rgba(117,155,143,0.1);
            transform: translateY(-3px);
        }

        .lp-persona-icon {
            font-size: 2rem;
            margin-bottom: 1rem;
        }

        .lp-persona-card h3 {
            font-size: 1.1rem;
            color: var(--lp-charcoal);
            margin-bottom: 0.6rem;
        }

        .lp-persona-card p {
            font-size: 0.9rem;
            color: var(--lp-text-light);
            line-height: 1.6;
        }

        /* ‚îÄ‚îÄ‚îÄ GUARANTEE ‚îÄ‚îÄ‚îÄ */
        .lp-guarantee {
