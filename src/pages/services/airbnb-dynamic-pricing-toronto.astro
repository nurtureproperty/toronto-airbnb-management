        .example-total { font-size: 1.5rem; font-weight: 700; padding-top: 1rem; margin-top: 0.5rem; border-top: 2px solid rgba(0,0,0,0.1); }
        .example-card.highlight .example-total { border-top-color: rgba(255,255,255,0.3); }

