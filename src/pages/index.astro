    <noscript><link rel="stylesheet" href="/styles/global.css"></noscript>

    <style is:inline>
        * { margin: 0; padding: 0; box-sizing: border-box; }

        :root {
            --primary: #759b8f;
            --primary-dark: #5a7d73;
            --accent: #d4a373;
            --cream: #ffffff;
            --warm-white: #FFFDF9;
            --charcoal: #2A2A2A;
            --shadow: rgba(0, 0, 0, 0.08);

            --lp-green: #759b8f;
            --lp-green-dark: #5a7d73;
            --lp-green-deeper: #4a6b61;
            --lp-cream: #ffffff;
            --lp-warm-white: #ffffff;
            --lp-charcoal: #2A2A2A;
            --lp-text: #3a3a3a;
            --lp-text-light: #6b6b6b;
            --lp-accent: #d4a373;
            --lp-red: #dc2626;
            --lp-green-positive: #16a34a;
            --lp-border: #e8e4dc;
        }

        html { scroll-behavior: smooth; }

        body {
            font-family: 'DM Sans', sans-serif;
            color: var(--charcoal);
            background: var(--cream);
            line-height: 1.6;
            overflow-x: hidden;
            -webkit-font-smoothing: antialiased;
        }

        h1, h2, h3, h4, h5, h6 {
            font-family: 'Poppins', sans-serif;
            font-weight: 600;
            line-height: 1.15;
        }

        /* Override critical.css main padding - hero handles its own spacing */
        main {
            padding-top: 0 !important;
        }

        /* ═══════════════════════════════════════
           LP SECTIONS
           ═══════════════════════════════════════ */

        /* ─── HERO ─── */
        .lp-hero {
            margin-top: 72px;
            padding: 0 2rem;
            background: white;
            position: relative;
            overflow: hidden;
        }

        .lp-hero-inner {
            max-width: 1400px;
            margin: 0 auto;
            display: grid;
            grid-template-columns: 1fr 1fr;
            column-gap: 4rem;
            row-gap: 0.5rem;
            align-items: center;
            position: relative;
        }

        .lp-hero-content h1 {
            font-size: 4rem;
            line-height: 1.1;
            margin-bottom: 1.5rem;
            color: var(--lp-charcoal);
        }

        .lp-hero-content .highlight {
            color: var(--lp-green-dark);
        }

        .lp-hero-description {
            font-size: 1.125rem;
            color: #666;
            margin-bottom: 1rem;
            line-height: 1.8;
            animation: fadeInUp 0.8s ease-out 0.4s both;
        }

        .lp-hero-stats {
            display: flex;
            gap: 3rem;
            margin-top: 0;
            margin-bottom: 1rem;
            animation: fadeInUp 0.8s ease-out 0.6s both;
            align-items: flex-end;
            grid-column: 1;
            grid-row: 2;
        }

        .lp-stat { text-align: left; }

        .lp-stat:not(.badge-stat) { margin-bottom: 13px; }

        .lp-stat-number {
            font-family: 'Poppins', sans-serif;
            font-size: 2rem;
            font-weight: 700;
            color: var(--lp-green-dark);
            display: block;
        }

        .lp-stat-label { color: #666; font-size: 0.9rem; }

        .badge-stat {
            display: flex;
            flex-direction: column;
            align-items: center;
        }

        .lp-badge-img {
            height: 120px;
            width: auto;
            object-fit: contain;
        }

        .lp-badge-guest-fav { margin-bottom: -10px; }

        .lp-hero-buttons {
            display: flex;
            gap: 1.5rem;
            margin-top: 1.5rem;
            justify-content: center;
            animation: fadeInUp 0.8s ease-out 0.8s both;
        }

        .lp-btn-primary {
            background: var(--lp-green);
            color: white;
            padding: 1.25rem 3rem;
            border-radius: 50px;
            text-decoration: none;
            font-weight: 600;
            font-size: 1.2rem;
            transition: all 0.3s;
            border: none;
            display: inline-flex;
            align-items: center;
            gap: 0.75rem;
            box-shadow: 0 8px 25px rgba(117, 155, 143, 0.4);
            cursor: pointer;
        }

        .lp-btn-primary::after {
            content: '→';
            font-size: 1.3rem;
            transition: transform 0.3s;
        }

        .lp-btn-primary:hover {
            background: var(--lp-green-dark);
            transform: translateY(-3px);
            box-shadow: 0 12px 35px rgba(117, 155, 143, 0.5);
        }

        .lp-btn-primary:hover::after { transform: translateX(4px); }

        /* Hero Video */
        .lp-hero-video {
            position: relative;
            animation: fadeInRight 1s ease-out 0.4s both;
            grid-column: 2;
            grid-row: 1 / 3;
        }

        .lp-video-wrapper {
            position: relative;
            width: 100%;
            padding-bottom: 56.25%;
            border-radius: 20px;
            overflow: hidden;
            box-shadow: 0 20px 60px rgba(0, 0, 0, 0.15);
            background: #000;
            cursor: pointer;
            touch-action: manipulation;
            -webkit-tap-highlight-color: transparent;
        }

        .lp-video-wrapper * { pointer-events: none; }

        .lp-video-wrapper img {
            position: absolute;
            top: 0; left: 0;
            width: 100%; height: 100%;
            object-fit: cover;
        }

        .lp-video-wrapper .play-btn {
            position: absolute;
            top: 50%; left: 50%;
            transform: translate(-50%, -50%);
            z-index: 1;
            background: transparent;
            border: none;
            padding: 0;
            cursor: pointer;
            transition: transform 0.2s ease, filter 0.2s ease;
        }

        .lp-video-wrapper:hover .play-btn {
            transform: translate(-50%, -50%) scale(1.1);
            filter: brightness(1.1);
        }

        .lp-video-wrapper.activated img,
        .lp-video-wrapper.activated .play-btn { display: none; }

        .lp-video-wrapper iframe {
            position: absolute;
            top: 0; left: 0;
            width: 100%; height: 100%;
            border: none;
        }

        /* ─── PLATFORM TRUST STRIP ─── */
        .lp-platforms {
            padding: 0.5rem 0;
            margin-top: 2rem;
            background: linear-gradient(180deg, rgba(117, 155, 143, 0.08) 0%, rgba(117, 155, 143, 0.04) 100%);
            border-top: 1px solid rgba(117, 155, 143, 0.15);
            border-bottom: 1px solid rgba(117, 155, 143, 0.15);
            overflow: hidden;
        }

        .lp-platforms-label {
            font-size: 0.8rem;
            font-weight: 600;
            text-transform: uppercase;
            letter-spacing: 0.08em;
            color: var(--lp-green-dark);
            text-align: center;
            margin-bottom: 1rem;
        }

        .lp-scroll-track {
            display: flex;
            animation: scrollLogos 18s linear infinite;
            will-change: transform;
        }

        .lp-scroll-set {
            display: flex;
            align-items: center;
            justify-content: space-around;
            min-width: 100vw;
            flex-shrink: 0;
        }

        .lp-platform-logo { flex-shrink: 0; }

        .lp-platform-logo img {
            height: 50px;
            width: 250px;
            opacity: 0.8;
            display: block;
        }

        @keyframes scrollLogos {
            0% { transform: translateX(0); }
            100% { transform: translateX(-100vw); }
        }

        /* ─── BOLD BANNER ─── */
        .lp-bold-banner {
            padding: 2.5rem 2rem;
            background: white;
            text-align: center;
        }

        .lp-bold-banner-inner {
            max-width: 900px;
            margin: 0 auto;
        }

        .lp-bold-banner h2 {
            font-size: clamp(1.5rem, 3.5vw, 2.25rem);
            color: var(--lp-charcoal);
            line-height: 1.35;
            margin-top: 0;
            margin-bottom: 0.5rem;
            font-weight: 600;
        }

        .lp-bold-banner h2 + h2 { margin-top: 0.75rem; }

        .lp-bold-banner .banner-help {
            font-size: clamp(1.5rem, 3.5vw, 2.25rem);
            color: var(--lp-green-dark);
            font-weight: 700;
            font-family: 'Poppins', sans-serif;
            display: inline-block;
            position: relative;
            margin-top: 0.75rem;
        }

        .banner-underline {
            position: absolute;
            bottom: -8px;
            left: 0;
            width: 100%;
            height: 12px;
            color: var(--lp-green);
        }

        .banner-story {
            color: #555;
            font-size: 1.05rem;
            line-height: 1.75;
            max-width: 750px;
            margin: 1rem auto 0;
        }

        /* ─── THE FLIP ─── */
        .lp-flip {
            padding: 5rem 2rem;
            background: var(--lp-charcoal);
            color: white;
            text-align: center;
        }

        .lp-flip-inner {
            max-width: 680px;
            margin: 0 auto;
        }

        .lp-flip h2 {
            font-size: clamp(1.75rem, 3.5vw, 2.5rem);
            margin-bottom: 1rem;
        }

        .lp-flip p {
            font-size: 1.15rem;
            opacity: 0.92;
            margin-bottom: 2rem;
            line-height: 1.7;
        }

        .lp-flip-benefits {
            list-style: none;
            padding: 0;
            margin: 0 auto 2rem;
            max-width: 520px;
            text-align: left;
        }

        .lp-flip-benefits li {
            padding: 0.5rem 0;
            padding-left: 1.75rem;
            position: relative;
            font-size: 1.05rem;
            opacity: 0.92;
            line-height: 1.5;
        }

        .lp-flip-benefits li::before {
            content: '✓';
            position: absolute;
            left: 0;
            color: var(--lp-green);
            font-weight: 700;
        }

        /* ─── CASE STUDY ─── */
        .lp-case {
            padding: 5.5rem 2rem;
            background: white;
            color: var(--lp-charcoal);
        }

        .lp-case-inner { max-width: 960px; margin: 0 auto; }

        .lp-case-label {
            text-align: center;
            font-size: 0.8rem;
            font-weight: 600;
            text-transform: uppercase;
            letter-spacing: 0.1em;
            color: var(--lp-green);
            margin-bottom: 0.5rem;
        }

        .lp-case h2 {
            text-align: center;
            font-size: clamp(1.75rem, 3.5vw, 2.5rem);
            color: var(--lp-charcoal);
            margin-bottom: 0.5rem;
        }

        .lp-case-subtitle {
            text-align: center;
            color: #888;
            margin-bottom: 3rem;
            font-size: 1.05rem;
        }

        .lp-condo-image {
            text-align: center;
            margin-bottom: 2.5rem;
            line-height: 0;
        }

        .lp-condo-image img {
            max-width: 700px;
            width: 100%;
            height: auto;
            display: inline-block;
            border-radius: 12px;
        }

        .lp-case-grid {
            display: grid;
            grid-template-columns: 1fr 1fr;
            gap: 3rem;
        }

        .lp-case-column-label {
            font-family: 'Poppins', sans-serif;
            font-size: 0.75rem;
            font-weight: 600;
            text-transform: uppercase;
            letter-spacing: 0.1em;
            color: #999;
            padding-bottom: 1rem;
            border-bottom: 1px solid #e5e5e5;
            margin-bottom: 0;
        }

        .lp-case-line {
            display: flex;
            justify-content: space-between;
            padding: 0.85rem 0;
            border-bottom: 1px solid #eee;
            font-size: 0.95rem;
        }

        .lp-case-line-label { font-weight: 500; color: #555; }
        .lp-case-line-value { font-weight: 400; color: var(--lp-charcoal); }

        .lp-case-line.total { border-bottom: none; padding-top: 1rem; }
        .lp-case-line.total .lp-case-line-label { font-weight: 700; color: var(--lp-charcoal); }
        .lp-case-line.total .lp-case-line-value { font-weight: 700; font-size: 1.15rem; }

        .lp-negative { color: #f87171; }
        .lp-positive { color: #4ade80; }

        .lp-case-result {
            text-align: center;
            margin-top: 3rem;
            padding-top: 2rem;
            border-top: 1px solid #e5e5e5;
        }

        .lp-case-result-number {
            font-family: 'Poppins', sans-serif;
            font-size: 2.5rem;
            font-weight: 700;
            color: var(--lp-green-dark);
            line-height: 1;
            margin-bottom: 0.35rem;
        }

        .lp-case-result-text { font-size: 0.95rem; color: #888; }

        /* ─── HOW IT WORKS ─── */
        .lp-how {
            padding: 5.5rem 2rem;
            background: var(--lp-charcoal);
            color: white;
        }

        .lp-section-inner { max-width: 1100px; margin: 0 auto; }

        .lp-section-header { text-align: center; margin-bottom: 3rem; }

        .lp-section-header h2 {
            font-size: clamp(1.75rem, 3.5vw, 2.5rem);
            color: var(--lp-charcoal);
            margin-bottom: 0.75rem;
        }

        .lp-section-header p {
            font-size: 1.1rem;
            color: var(--lp-text-light);
            max-width: 600px;
            margin: 0 auto;
        }

        .lp-section-label {
            font-size: 0.8rem;
            font-weight: 600;
            text-transform: uppercase;
            letter-spacing: 0.1em;
            color: var(--lp-green-dark);
            margin-bottom: 0.5rem;
        }

        .lp-how .lp-section-header h2 { color: white; }
        .lp-how .lp-section-header p { color: rgba(255, 255, 255, 0.8); }
        .lp-how .lp-section-label { color: var(--lp-green); }

        .lp-how-steps {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            gap: 2rem;
            margin-top: 0.5rem;
        }

