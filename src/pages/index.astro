    <noscript><link href="https://fonts.googleapis.com/css2?family=Poppins:wght@400;600;700&family=DM+Sans:wght@0,400;0,500;0,600;0,700&display=swap" rel="stylesheet"></noscript>

    <!-- Critical CSS - Inlined for fast initial render (includes header/footer styles) -->
